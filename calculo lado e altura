#include <stdio.h>

int main() {
    float L, H, area;

    // Solicita ao usuário que insira os valores de L e H
    printf("Insira o valor do lado (L): ");
    scanf("%f", &L);

    printf("Insira o valor da altura (H): ");
    scanf("%f", &H);

    // Calcula a área do retângulo
    area = L * H;

    // Exibe o resultado
    printf("A área do retângulo é: %.2f\n", area);

    return 0;
}
